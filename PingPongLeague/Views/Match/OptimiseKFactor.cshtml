@model IEnumerable<EloRating.KFactorResult>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.KFactor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AverageError)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.KFactor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AverageError)
            </td>
       
        </tr>
    }

</table>
